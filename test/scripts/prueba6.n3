@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix se: <http://www.sanger.ac.uk/ontologies/lims/se.owl#> .
@prefix instance: <http://localhost:3000/labware/> .

@forAll :a, :b, :c .
{ 
  ?plate se:a "Plate" .
  ?plate se:contains ?tube .
  ?tube log:semantics ?graph .
  ?graph log:includes {?a ?b ?c . }.
} => { ?a ?b ?c . }.

@forAll :x, :y, :location.
{ 
  ?x se:a """Well""" .
  ?x se:location ?location .
  ?y se:a """Well""" .
  ?y se:location ?location .
  ?x log:notEqualTo ?y .
} => {
  se:step se:addFacts {(?x ?y) se:location ?location .}.
}.
